{
	"name": "langly",
	"version": "0.0.14",
	"description": "Tiny language and localization library.",
	"author": "Utily Contributors",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/payfunc/langly"
	},
	"bugs": {
		"url": "https://github.com/payfunc/langly/issues"
	},
	"homepage": "https://payfunc.com/open-source/langly",
	"license": "MIT",
	"private": false,
	"main": "dist/index.js",
	"typings": "dist/index.d.ts",
	"git": {
		"tagName": "v${version}"
	},
	"github": {
		"release": true
	},
	"jest": {
		"globals": {
			"ts-jest": {
				"tsConfig": "tsconfig.test.json"
			}
		},
		"testEnvironment": "node",
		"testRegex": "((\\.|/)(test|spec))(\\.|\\/.+)(jsx?|tsx?)$",
		"testPathIgnorePatterns": [
			"node_modules/",
			"dist/"
		],
		"collectCoverageFrom": [
			"**/*.{ts,tsx,js,jsx}",
			"!**/node_modules/**",
			"!**/dist/**"
		],
		"preset": "ts-jest",
		"testMatch": null
	},
	"scripts": {
		"build": "tsc -p .",
		"lint": "eslint '**/*.{js,ts,tsx}'",
		"fix": "eslint '**/*.{js,ts,tsx}' --fix",
		"pretest": "npm run build",
		"test": "jest",
		"prepare": "npm run build",
		"clean": "rm -rf dist node_modules coverage"
	},
	"dependencies": {
		"isoly": "0.0.47"
	},
	"devDependencies": {
		"@types/jest": "^26.0.0",
		"@types/node": "^14.0.13",
	"@typescript-eslint/eslint-plugin": "^5.23.0",
	"@typescript-eslint/parser": "^5.23.0",
		"babel-core": "^6.26.3",
		"babel-jest": "^26.0.1",
		"eslint": "^8.4.1",
		"eslint-plugin-prettierx": "github:utily/eslint-plugin-prettierx#utily-20220323",
		"eslint-plugin-simple-import-sort": "^7.0.0",
		"jest": "^26",
		"prettierx": "github:utily/prettierx#utily-20220323",
		"ts-jest": "^26.1.0",
	"typescript": "^4.4.3"
	}
}
